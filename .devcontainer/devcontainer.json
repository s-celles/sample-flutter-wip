{
    "name": "Flutter Development",
    "build": {
        "dockerfile": "Dockerfile",
        "args": {
            "FLUTTER_VERSION": "3.27.1",
            "ANDROID_SDK_VERSION": "33",
            "ANDROID_BUILD_TOOLS_VERSION": "33.0.0"
        }
    },
    "customizations": {
        "vscode": {
            "extensions": [
                "dart-code.flutter",
                "dart-code.dart-code",
                "alexisvt.flutter-snippets",
                "nash.awesome-flutter-snippets"
            ]
        }
    },
    "remoteUser": "vscode",
    "containerEnv": {
        "CHROME_EXECUTABLE": "/mnt/c/Program Files (x86)/Google/Chrome/Application/chrome.exe"
    }
}